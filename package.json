{
  "name": "mangaweb",
  "version": "1.0.0",
  "description": "`mangaweb` is a web application which let the user read a manga from zip file without extracting it. Basically it scans for files in a designated directory and display \r it as a web application.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npx rimraf ./static",
    "build": "parcel build --dist-dir ./static ./static-src/index.js ./static-src/browse.ts ./static-src/view.ts ./static-src/tag_list.ts",
    "dev": "parcel watch --dist-dir ./static ./static-src/index.js ./static-src/browse.ts ./static-src/view.ts ./static-src/tag_list.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wutipong/mangaweb.git"
  },
  "author": "Wutipong Wongsakuldej",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wutipong/mangaweb/issues"
  },
  "homepage": "https://github.com/wutipong/mangaweb#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.9.2",
    "@types/bootstrap": "^5.2.6",
    "parcel": "^2.9.2",
    "parcel-transformer-svelte3-plus": "^0.2.9",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "bootswatch": "^5.3.0"
  }
}
